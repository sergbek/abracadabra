<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android"
       xmlns:tools="http://schemas.android.com/tools"
       xmlns:app="http://schemas.android.com/apk/res-auto"
       android:layout_width="match_parent"
       android:layout_height="match_parent"
       android:gravity="center"
       android:orientation="vertical"
       tools:background="@drawable/img_home_bg"
       tools:parentTag="LinearLayout">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:orientation="horizontal">

        <com.myboxandroid.ui.custom.HoneyCombView
            android:id="@+id/first"
            app:image="@drawable/ic_cat_dev_big_act"
            style="@style/HoneyCombStyle"
            app:text="TEST\nSERVICE"
            app:textSize="13sp"
            android:layout_width="@dimen/homey_comb_with_six_element"
            android:layout_height="wrap_content"/>

        <android.support.v4.widget.Space
            android:layout_width="5dp"
            android:layout_height="0dp"/>

        <com.myboxandroid.ui.custom.HoneyCombView
            android:id="@+id/second"
            app:image="@drawable/ic_cat_dev_big_act"
            style="@style/HoneyCombStyle"
            app:text="TEST\nSERVICE"
            app:textSize="13sp"
            android:layout_width="@dimen/homey_comb_with_six_element"
            android:layout_height="wrap_content"/>

    </LinearLayout>


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="-30dp"
        android:gravity="center"
        android:orientation="horizontal">

        <com.myboxandroid.ui.custom.HoneyCombView
            android:id="@+id/sixth"
            android:layout_width="@dimen/homey_comb_with_six_element"
            app:image="@drawable/ic_cat_dev_big_act"
            style="@style/HoneyCombStyle"
            app:text="TEST\nSERVICE"
            app:textSize="13sp"
            android:layout_height="wrap_content"/>


        <android.support.v4.widget.Space
            android:layout_width="@dimen/middle_space"
            android:layout_height="0dp"/>

        <com.myboxandroid.ui.custom.HoneyCombView
            android:id="@+id/third"
            android:layout_width="@dimen/homey_comb_with_six_element"
            app:image="@drawable/ic_cat_dev_big_act"
            style="@style/HoneyCombStyle"
            app:text="TEST\nSERVICE"
            app:textSize="13sp"
            android:layout_height="wrap_content"/>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="-30dp"
        android:gravity="center"
        android:orientation="horizontal">

        <com.myboxandroid.ui.custom.HoneyCombView
            android:id="@+id/fifth"
            android:layout_width="@dimen/homey_comb_with_six_element"
            app:image="@drawable/ic_cat_dev_big_act"
            style="@style/HoneyCombStyle"
            app:text="TEST\nSERVICE"
            app:textSize="13sp"
            android:layout_height="wrap_content"/>

        <android.support.v4.widget.Space
            android:layout_width="5dp"
            android:layout_height="0dp"/>

        <com.myboxandroid.ui.custom.HoneyCombView
            android:id="@+id/fourth"
            app:image="@drawable/ic_cat_dev_big_act"
            style="@style/HoneyCombStyle"
            app:text="TEST\nSERVICE"
            app:textSize="13sp"
            android:layout_width="@dimen/homey_comb_with_six_element"
            android:layout_height="wrap_content"/>

    </LinearLayout>

</merge>
