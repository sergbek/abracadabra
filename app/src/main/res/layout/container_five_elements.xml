<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android"
       xmlns:app="http://schemas.android.com/apk/res-auto"
       xmlns:tools="http://schemas.android.com/tools"
       android:layout_width="match_parent"
       android:layout_height="match_parent"
       android:gravity="center"
       android:orientation="vertical"
       tools:background="@drawable/img_home_bg"
       tools:parentTag="LinearLayout">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:orientation="horizontal">

        <com.myboxandroid.ui.custom.HoneyCombView
            android:id="@+id/first"
            style="@style/HoneyCombStyle"
            android:layout_width="@dimen/homey_comb_with"
            android:layout_height="wrap_content"
            app:image="@drawable/ic_cat_dev_big_act"
            app:text="TEST\nSERVICE"
            app:textSize="13sp"/>

        <android.support.v4.widget.Space
            android:layout_width="5dp"
            android:layout_height="0dp"/>

        <com.myboxandroid.ui.custom.HoneyCombView
            android:id="@+id/second"
            style="@style/HoneyCombStyle"
            android:layout_width="@dimen/homey_comb_with"
            android:layout_height="wrap_content"
            app:image="@drawable/ic_cat_dev_big_act"
            app:text="TEST\nSERVICE"
            app:textSize="13sp"/>

    </LinearLayout>


    <com.myboxandroid.ui.custom.HoneyCombView
        android:id="@+id/third"
        style="@style/HoneyCombStyle"
        android:layout_width="@dimen/homey_comb_with"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_marginTop="-35dp"
        app:image="@drawable/ic_cat_dev_big_act"
        app:text="TEST\nSERVICE"
        app:textSize="13sp"/>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="-35dp"
        android:gravity="center"
        android:orientation="horizontal">

        <com.myboxandroid.ui.custom.HoneyCombView
            android:id="@+id/fourth"
            style="@style/HoneyCombStyle"
            android:layout_width="@dimen/homey_comb_with"
            android:layout_height="wrap_content"
            app:image="@drawable/ic_cat_dev_big_act"
            app:text="TEST\nSERVICE"
            app:textSize="13sp"/>

        <android.support.v4.widget.Space
            android:layout_width="5dp"
            android:layout_height="0dp"/>

        <com.myboxandroid.ui.custom.HoneyCombView
            android:id="@+id/fifth"
            style="@style/HoneyCombStyle"
            android:layout_width="@dimen/homey_comb_with"
            android:layout_height="wrap_content"
            app:image="@drawable/ic_cat_dev_big_act"
            app:text="TEST\nSERVICE"
            app:textSize="13sp"/>

    </LinearLayout>

</merge>
